{
    "openapi":"3.0.2",
    "info": {
      "title":"Personal Finance Tracker",
      "version":"1.0",
      "description": "Personal Finance Tracker adalah aplikasi yang dirancang untuk membantu pengguna mengelola keuangan pribadi mereka. Aplikasi ini memungkinkan pengguna untuk mencatat pendapatan dan pengeluaran, membuat anggaran, dan menganalisis keuangan mereka melalui berbagai laporan dan grafik. Tujuannya adalah membantu pengguna memahami kebiasaan pengeluaran mereka dan membuat keputusan keuangan yang lebih baik.",
      "contact": {
        "name": "Irzi Rahmatullah",
        "email": "irzirahmatullah@gmail.com",
        "url": "-"
      }
    },
    "servers": [
        {"url": "https://localhost:3000/api"}
    ],
    "paths": {
        "/dashboard": {
            "get": {
                "description": "Getting accountID/wallet users data",
                "tags": ["dashboard"],
                "responses": {
                    "200": {
                        "description": "success getting data",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "accountID": {
                                                "type": "string",
                                                "nullable": false
                                            },
                                            "userID": {
                                                "type": "string",
                                                "nullable": false
                                            },
                                            "name": {
                                                "type": "string",
                                                "nullable": false
                                            },
                                            "amount": {
                                                "type": "number",
                                                "nullable": false
                                            },
                                            "category": {
                                                "type": "string",
                                                "nullable": false
                                            }
                                        }
                                    }
                                },
                                "example": [
                                    {
                                        "accountID": "zxcvbnm",
                                        "userID": "zirrah123",
                                        "name": "Uang Pribadi",
                                        "amount": 500000
                                    },
                                    {
                                        "accountID": "qwertyuiop",
                                        "userID": "zirrah123",
                                        "name": "Uang Makan",
                                        "amount": 10000000

                                    }
                                ]
                            }
                        }
                    },
                    "400": {
                        "description": "bad request",
                        "content": {
                            "application/json": {
                                "example": {
                                    "errors": "bad request"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/transactions": {
            "get": {
                "description": "Getting all transactions",
                "tags": ["transactions"],
                "responses": {
                    "200": {
                        "description": "success getting transactions",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "transactionID": {
                                                "type": "string",
                                                "nullable": false
                                            },
                                            "accountID": {
                                                "type": "string",
                                                "nullable": false
                                            },
                                            "type": {
                                                "type": "string",
                                                "nullable": false
                                            },
                                            "amount": {
                                                "type": "number",
                                                "nullable": false
                                            },
                                            "date": {
                                                "type": "string",
                                                "nullable": false
                                            },
                                            "description": {
                                                "type": "string",
                                                "nullable": true
                                            }
                                        }
                                    }
                                },
                                "example": [
                                    {
                                        "transactionID": "qwertyuiop",
                                        "accountID": "uniqueAccountID",
                                        "type": "income",
                                        "amount": 100000,
                                        "date": "timestamp",
                                        "description": "Transfer mamah"
                                    },
                                    {
                                        "transactionID": "zxcvbnm",
                                        "accountID": "uniqueAccountID",
                                        "type": "expense",
                                        "amount": 50000,
                                        "date": "timestamp",
                                        "description": "Beli hotwheel"
                                    }
                                ]
                            }
                        }
                    },
                    "400": {
                        "description": "bad request",
                        "content": {
                            "application/json": {
                                "example": {
                                    "errors": "Bad request"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "description": "create new transactions",
                "tags": ["transactions"],
                "responses": {
                    "200": {
                        "description": "Success create new transaction",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "userID": {
                                            "type": "string",
                                            "nullable": false
                                        },
                                        "accountID": {
                                            "type": "string",
                                            "nullable": false
                                        },
                                        "type": {
                                            "type": "string",
                                            "nullable": false
                                        },
                                        "amount": {
                                            "type": "number",
                                            "nullable": false
                                        },
                                        "date": {
                                            "type": "string",
                                            "nullable": false
                                        },
                                        "description": {
                                            "type": "string",
                                            "nullable": true
                                        }
                                    }
                                },
                                "example": [
                                    {
                                        "userID": "zirrah123",
                                        "accountID": "uniqueAccountID",
                                        "type": "income",
                                        "amount": 50000,
                                        "date": "timestamp",
                                        "description": "transfer"
                                    },
                                    {
                                        "userID": "zirrah123",
                                        "accountID": "uniqueAccountID",
                                        "type": "expense",
                                        "amount": 10000,
                                        "date": "timestamp",
                                        "description": "beli uduk"
                                    }
                                ]
                            }
                        }
                    },
                    "400": {
                        "description": "Failed to create new transaction",
                        "content": {
                            "application/json": {
                                "example": {
                                    "errors": "Bad request"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/reports": {          
            "get": {
                "description": "Getting reports income and expense data",
                "tags": ["reports"],
                "responses": {
                    "200": {
                        "description": "Success getting data",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "transactionID": {
                                                "type": "string",
                                                "nullable": false
                                            },
                                            "accountID": {
                                                "type": "string",
                                                "nullable": false
                                            },
                                            "type": {
                                                "type": "string",
                                                "nullable": false
                                            },
                                            "amount": {
                                                "type": "number",
                                                "nullable": false
                                            },
                                            "date": {
                                                "type": "string",
                                                "nullable": false
                                            },
                                            "description": {
                                                "type": "string",
                                                "nullable": true
                                            }
                                        }
                                    }
                                },
                                "example": [
                                    {
                                        "transactionID": "qwertyuiop",
                                        "accountID": "uniqueAccountID",
                                        "type": "income",
                                        "amount": 100000,
                                        "date": "timestamp",
                                        "description": "Transfer mamah"
                                    },
                                    {
                                        "transactionID": "zxcvbnm",
                                        "accountID": "uniqueAccountID",
                                        "type": "expense",
                                        "amount": 50000,
                                        "date": "timestamp",
                                        "description": "Beli hotwheel"
                                    }
                                ]
                            }
                        }
                    },
                    "400": {
                        "description": "bad request",
                        "content": {
                            "application/json": {
                                "example": {
                                    "errors": "Bad request"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}